{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/send-message",
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/webhook2",
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
